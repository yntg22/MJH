<script type="text/javascript">
//객체 정의
var rect = {
		width : 23,
		height : 12,
		
		getArea : function(){
			area = this.width * this.height;
			return area;		
		},
		getCircum : function(){
			circum = (this.width + this.height) * 2;
			return circum;
		}
}



</script>
